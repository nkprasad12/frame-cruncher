{
  "name": "frame-cruncher",
  "version": "1.0.0",
  "description": "Utility to help Melee players analyze their replay files.",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf genfiles",
    "build-ts": "tsc -p tsconfig.json",
    "build-cli": "npm run clean && npm run build-ts",
    "build": "npx webpack --config webpack.config.js",
    "site": "npm run build && xdg-open genfiles/main.html",
    "analyze": "npm run build-cli && node genfiles/cli/run.js"
  },
  "author": "nkprasad12",
  "license": "ISC",
  "dependencies": {
    "@slippi/slippi-js": "^6.1.0",
    "typescript": "^4.3.2"
  },
  "devDependencies": {
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "html-webpack-plugin": "^5.3.1",
    "prettier": "2.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "webpack": "^5.39.1",
    "webpack-cli": "^4.7.2"
  }
}
